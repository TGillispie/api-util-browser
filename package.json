{
  "name": "api-util-browser",
  "version": "0.1.0",
  "type": "module",
  "description": "This program is a utility for testing APIs in **Browser** JavaScript.  It Shadows a NodeJS version for JS comparison.",
  "main": "index.mjs",
  "dependencies": {
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "jasmine-browser-runner": "^1.0.0",
    "jasmine-core": "^4.1.1",
    "live-server": "^1.2.2"
  },
  "devDependenciesNotes": {
    "jasmine-core": [
			"5/26/2022: requires nodeJS < 18.0 due to selenium bug/patch",
  		"https://github.com/SeleniumHQ/selenium/issues/10619"
		]
  },
  "scripts": {
    "test": "jasmine-browser-runner runSpecs --config=test/support/jasmine-browser.json --color --browser=headlessChrome",
    "test-serve": "jasmine-browser-runner serve --config=test/support/jasmine-browser.json",
    "dev": "live-server --no-browser --cors --port=3006"
  },
  "author": "Tyler Gillispie",
  "license": "ISC"
}
